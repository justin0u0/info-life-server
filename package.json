{
  "name": "info-life-server",
  "version": "1.0.0",
  "description": "2020 Software Studio Final Project InfoLife API Server",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.2.0",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-info-life-server",
    "stop": "egg-scripts stop --title=egg-server-info-life-server",
    "dev": "docker-compose -f ./docker-compose.dev.yml up -d && egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "dc:up": "docker-compose -f ./docker-compose.dev.yml up -d"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "justin0u0",
  "license": "MIT"
}
